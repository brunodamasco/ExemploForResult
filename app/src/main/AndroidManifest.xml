<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.senac.renato.exemploforresult">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.EnderecoActivity"
            android:label="@string/titulo_endereco" />
        <activity
            android:name=".view.DadosPessoaisActivity"
            android:label="@string/titulo_dados_pessoais" />
        <activity
            android:name=".view.ResultadoActivity"
            android:label="@string/titulo_resultado" />
        <activity android:name=".view.ContatoActivity"
            android:label="Contato"/>
    </application>

</manifest>